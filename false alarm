#include <iostream>
using namespace std;  
int main() {
    int t;
    cin >> t;

    while (t--) {
        int n, x;
        cin >> n >> x;
        int doors[10];
        for (int i = 0; i < n; i++) {
            cin >> doors[i];
        }  int o=0;
        int u=x;
        int t=0; int y=0;
         for (int i = 0; i < n; i++) {
            if (u>0){
          if (doors[i]==1&&t==0) {u--; t++;}
          else if(t!=0) u--;
        }
      
        else if (u==0&&doors[i]==1){ cout <<"NO"<<endl;y++ ;break;}
        else if (u==0&&doors[i]==0) o++;

    }
if (y==0){
if (o!=0||x!=u) cout<<"YES"<<endl;

}







    
}}
